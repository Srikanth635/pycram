  <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/SOMA-HOME.owl"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A topic relevant for a think-aloud communication.</rdfs:comment>
  <rdfs:label xml:lang="en">Think aloud topic</rdfs:label>
</owl:Class>

<owl:Class rdf:about="http://www.ease-crc.org/ont/SOMA.owl#MetacognitiveControlling">
