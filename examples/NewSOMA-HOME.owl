  <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/SOMA-ACT.owl"/>
  <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/SOMA-HOME.owl"/>
  <rdfs:comment xml:lang="en">A mental task, in which an agent reconfigures some plan that has been put together before.</rdfs:comment>
  <rdfs:label xml:lang="en">Replanning</rdfs:label>
</owl:Class>

<owl:Class rdf:about="http://www.ease-crc.org/ont/SOMA.owl#RevoluteJoint">
