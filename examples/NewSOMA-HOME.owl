  <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Description"/>
  <rdfs:range rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#ProcessType"/>
  <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/SOMA-HOME.owl"/>
  <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/SOMA-PROC.owl"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relation between a description and a process type.</rdfs:comment>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">defines process</rdfs:label>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#isProcessDefinedIn">
  <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isDefinedIn"/>
  <rdfs:domain rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#ProcessType"/>
  <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Description"/>
  <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/SOMA-HOME.owl"/>
  <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/SOMA-PROC.owl"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relation between a process type and a description that defines it.</rdfs:comment>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">is process defined in</rdfs:label>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#isSetpointDefinedIn">
  <rdfs:subPropertyOf rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#isParameterDefinedIn"/>
  <rdfs:domain rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#Setpoint"/>
  <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Description"/>
  <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/SOMA-HOME.owl"/>
  <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/SOMA-OBJ.owl"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines the dedicated goal region of a description.</rdfs:comment>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">is setpoint defined in</rdfs:label>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#isTriggerDefinedIn">
  <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isRoleDefinedIn"/>
  <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Role"/>
  <rdfs:range rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#Affordance"/>
  <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/SOMA-HOME.owl"/>
  <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/SOMA-OBJ.owl"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Relates an affordance which is a relation between a bearer and a trigger, to the role of the trigger when the affordance is manifested.</rdfs:comment>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">is trigger defined in</rdfs:label>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#derivedFrom">
  <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/SOMA-HOME.owl"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#describesQuality">
  <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#describes"/>
