    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/SOMA-HOME.owl"/>
  <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/SOMA-OBJ.owl"/>
  <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/SOMA-STATE.owl"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Classifies States in which an object is placed inside another.</rdfs:comment>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The disposition of an object (the container) to contain others.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://www.ease-crc.org/ont/SOMA.owl#IncludedObject">
