  <rdfs:domain rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#MotionProcess"/>
  <rdfs:range rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#Motion"/>
  <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/SOMA-PROC.owl"/>
  <rdfs:comment xml:lang="en">A relation between an motion process and a motion event type, e.g. 'lifting an object' executes the motion process 'lifting'.</rdfs:comment>
  <rdfs:label xml:lang="en">executes motion</rdfs:label>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#isOccurrenceOf">
  <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/SOMA-HOME.owl"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#isExecutedMotionIn">
  <rdfs:subPropertyOf rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#isOccurringIn"/>
  <rdfs:domain rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#Motion"/>
  <rdfs:range rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#MotionProcess"/>
  <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/SOMA-PROC.owl"/>
  <rdfs:comment xml:lang="en">A relation between an motion process and a motion event type, e.g. 'lifting an object' executes the motion process 'lifting'.</rdfs:comment>
  <rdfs:label xml:lang="en">is executed motion in</rdfs:label>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#finishedBy">
  <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/SOMA-HOME.owl"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#finishes">
  <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/SOMA-HOME.owl"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#firstMember">
  <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/SOMA-HOME.owl"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#givesMeaningTo">
  <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#associatedWith"/>
  <owl:inverseOf rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#isGivenMeaningBy"/>
  <rdfs:domain rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#System"/>
  <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#InformationObject"/>
  <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/SOMA-HOME.owl"/>
  <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/SOMA-IO.owl"/>
  <rdfs:comment xml:lang="en">The relation between a System and Information Object that is given meaning to by said system, e.g., a Language might give meaning to some word, sentence, text, etc., but without the knowledge of said System (Language), the text will not make sense to a reader.</rdfs:comment>
  <rdfs:label xml:lang="en">gives meaning to</rdfs:label>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#isGivenMeaningBy">
  <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#associatedWith"/>
