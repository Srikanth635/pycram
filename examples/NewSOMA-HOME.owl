  <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/SOMA-HOME.owl"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A component designed to control the flow of water from a pipe.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://www.ease-crc.org/ont/SOMA.owl#Tapping">
