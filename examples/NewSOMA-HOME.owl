</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isLocationOf">
  <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/SOMA-HOME.owl"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#isInsideOf">
