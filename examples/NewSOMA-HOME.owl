  <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/SOMA-OBJ.owl"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A role classifying the object that is the outcome of a creation or modification action or process.</rdfs:comment>
  <rdfs:label xml:lang="en">Result role</rdfs:label>
