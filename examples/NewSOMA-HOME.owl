    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/SOMA-ACT.owl"/>
  <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/SOMA-HOME.owl"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A task in which an agent dissolves small particles like sugar or salt in fluid</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://www.ease-crc.org/ont/SOMA.owl#Storage">
