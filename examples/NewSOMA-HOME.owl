                      <rdf:rest>
                        <rdf:Description>
                          <rdf:first rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#realizes"/>
                          <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
