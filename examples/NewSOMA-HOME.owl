  <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/SOMA-HOME.owl"/>
  <rdfs:comment xml:lang="en">The role played by an Agent in a Communication Task that perceives and interpretes some incoming Message.</rdfs:comment>
  <rdfs:label xml:lang="en">Receiver</rdfs:label>
</owl:Class>

<owl:Class rdf:about="http://www.ease-crc.org/ont/SOMA.owl#Sender">
