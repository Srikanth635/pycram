  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Friction between two touching objects that do not move relative to each other.</rdfs:comment>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Static friction attribute</rdfs:label>
</owl:Class>

<owl:Class rdf:about="http://www.ease-crc.org/ont/SOMA.owl#Status">
