  <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Quality"/>
  <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Description"/>
  <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/SOMA-HOME.owl"/>
  <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/SOMA-OBJ.owl"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Relates a description to a quality it describes.</rdfs:comment>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">is quality described by</rdfs:label>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#directlyCauses">
  <rdfs:subPropertyOf rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#causes"/>
  <owl:inverseOf rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#isDirectReactionTo"/>
  <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Event"/>
  <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Event"/>
  <owl:propertyChainAxiom>
    <rdf:Description>
      <rdf:first rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#causesTransition"/>
      <rdf:rest>
        <rdf:Description>
          <rdf:first rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#hasTerminalScene"/>
          <rdf:rest>
            <rdf:Description>
              <rdf:first rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#includesEvent"/>
              <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
            </rdf:Description>
          </rdf:rest>
        </rdf:Description>
      </rdf:rest>
    </rdf:Description>
  </owl:propertyChainAxiom>
  <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/SOMA-ACT.owl"/>
  <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/SOMA-HOME.owl"/>
  <rdfs:comment xml:lang="en">Non-transitive version of "causes".</rdfs:comment>
  <rdfs:label xml:lang="en">directly causes</rdfs:label>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#isDirectReactionTo">
  <rdfs:subPropertyOf rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#isReactionTo"/>
  <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Event"/>
  <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Event"/>
  <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/SOMA-ACT.owl"/>
  <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/SOMA-HOME.owl"/>
  <rdfs:comment xml:lang="en">Non-transitive version of "is reaction to".</rdfs:comment>
  <rdfs:label xml:lang="en">is direct reaction to</rdfs:label>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#hasTerminalScene">
  <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/SOMA-HOME.owl"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#includesEvent">
  <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/SOMA-HOME.owl"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#directlyDerivedFrom">
  <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/SOMA-HOME.owl"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#during">
  <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/SOMA-HOME.owl"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#encapsulates">
